<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>


<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
            xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.davidflex.supermarket.gui.PersonalAgentGuiController">
   <padding>
      <Insets bottom="10.0"/>
   </padding>
   <VBox layoutX="10.0" layoutY="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0"
         AnchorPane.topAnchor="10.0">
      <TableView fx:id="table" prefHeight="200.0" prefWidth="200.0">
         <columns>
            <TableColumn fx:id="productCol" maxWidth="4000.0" minWidth="80.0" prefWidth="127.0" text="Product"/>
            <TableColumn fx:id="categoryCol" maxWidth="3000.0" minWidth="40.0" prefWidth="60.0" text="Category"/>
            <TableColumn fx:id="quantityCol" maxWidth="3000.0" text="Quantity"/>
            <TableColumn fx:id="maxPriceCol" maxWidth="3000.0" text="Max. Price"/>
            <TableColumn fx:id="price" maxWidth="3000.0" text="Price"/>
            <TableColumn fx:id="status" minWidth="140.0" prefWidth="111.0" text="Status"/>
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
         </columnResizePolicy>
      </TableView>
      <Label text="Status:"/>
      <TextArea fx:id="statusArea" prefHeight="101.0" prefWidth="680.0"/>
   </VBox>
</AnchorPane>
